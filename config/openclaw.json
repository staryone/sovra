{
  "$schema": "https://openclaw.ai/config-schema.json",
  "version": "1.0",
  "agent": {
    "name": "Sovra",
    "description": "A Sovereign & Self-Evolving AI Agent",
    "model": {
      "provider": "ollama",
      "endpoint": "http://localhost:11434",
      "model_name": "sovra-brain",
      "api_key": "ollama-local",
      "context_length": 65536
    }
  },
  "skills": {
    "enabled": [
      "shell",
      "file_manager",
      "web_browser",
      "web_search",
      "code_runner",
      "package_manager"
    ],
    "disabled": [
      "email",
      "calendar",
      "social_media"
    ]
  },
  "messaging": {
    "platforms": {
      "telegram": {
        "enabled": false,
        "bot_token": "",
        "allowed_user_ids": []
      },
      "discord": {
        "enabled": false,
        "bot_token": "",
        "allowed_server_ids": []
      },
      "whatsapp": {
        "enabled": false
      },
      "web": {
        "enabled": true,
        "port": 3000
      }
    }
  },
  "security": {
    "sandbox_mode": false,
    "allowed_directories": ["/"],
    "max_command_timeout_seconds": 300,
    "blocked_commands": ["rm -rf /", ":(){ :|:& };:", "dd if=/dev/zero of=/dev/sda"]
  },
  "memory": {
    "enabled": true,
    "persistence_path": "./data/openclaw_memory"
  }
}
