{
  "agents": {
    "defaults": {
      "model": {
        "primary": "ollama/sovra-brain"
      },
      "models": {
        "ollama/sovra-brain": {
          "alias": "Sovra",
          "params": {
            "temperature": 0.7,
            "maxTokens": 2048
          }
        }
      },
      "workspace": "/root/.openclaw/workspace",
      "timeoutSeconds": 300,
      "maxConcurrent": 2,
      "contextTokens": 8192
    }
  },
  "models": {
    "mode": "merge",
    "providers": {
      "ollama": {
        "baseUrl": "http://127.0.0.1:11434/v1",
        "apiKey": "ollama",
        "api": "openai-completions",
        "models": [
          {
            "id": "sovra-brain",
            "name": "SOVRA Brain (Qwen2.5 1.5B)",
            "reasoning": false,
            "input": ["text"],
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 8192,
            "maxTokens": 2048
          }
        ]
      }
    }
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "botToken": "${TELEGRAM_BOT_TOKEN}",
      "dmPolicy": "pairing",
      "groups": {
        "*": { "requireMention": true }
      },
      "historyLimit": 20,
      "streamMode": "partial"
    }
  },
  "gateway": {
    "mode": "local",
    "port": 18789,
    "bind": "loopback",
    "auth": {
      "mode": "token",
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    },
    "nodes": {
      "denyCommands": [
        "camera.snap",
        "camera.clip",
        "screen.record",
        "calendar.add",
        "contacts.add",
        "reminders.add"
      ]
    }
  },
  "messages": {
    "ackReactionScope": "group-mentions"
  }
}
